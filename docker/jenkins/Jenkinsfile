pipeline {
  agent { node { label 'finn-build' } }
  stages {
    stage('Quicktest') {
      steps {
        sh 'echo "Hello FINN"'
        sh 'hostname'
        sh 'whoami'
        sh 'pwd'
        sh 'docker login'
        sh 'printenv | sort'
        sh './run-docker.sh quicktest'
      }
    }
  }
}
