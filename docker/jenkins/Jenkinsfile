pipeline {
  agent { node { label 'finn-build' } }
   environment {
    FINN_XILINX_PATH="/proj/xbuilds/SWIP/2022.1_0420_0327/installs/lin64"
    FINN_XILINX_VERSION="2022.1"
    FINN_DOCKER_TAG="xilinx/finn:jenkins"
    FINN_HOST_BUILD_DIR="/scratch/users/finn_ci"
    PLATFORM_REPO_PATHS="/opt/xilinx/platforms"
  }
  stages {
    stage('Quicktest') {
      steps {
        sh 'echo "Hello FINN"'
        sh 'hostname'
        sh 'whoami'
        sh 'pwd'
        sh 'docker login'
        sh 'printenv | sort'
        sh './run-docker.sh quicktest'
      }
    }
  }
}
